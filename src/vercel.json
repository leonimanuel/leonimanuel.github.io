{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://wide-angle.ghost.io"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://wide-angle.ghost.io;"
        }
      ]
    }
  ]
}
